<h3>哪些产品在使用？</h3>

<%_ for (const products of theme.products) {_%>
<a href="<%- products.url %>" target="_blank" style="max-width: 160px;">
  <img src="<%- url_for(`/img/products/${products.img}`) %>" style=" " alt="<%-products.name-%>">
</a>
<%_ } _%>


<script>
window.addEventListener('load', function () {
  [].forEach.call(document.querySelectorAll('.open-collective-sponsors img'), function (img) {
    if (img.width === 1) {
      img.width = 0
      img.parentElement.style.margin = '0 -1px 0 0'
    }
  })
})
</script>

<br>
<br>
<br>

<h3>云计算赞助商</h3>
<%_ for (const sponsor of theme.cloud_sponsors) {_%>
<a href="<%- sponsor.url %>" target="_blank" style="width: 120px;">
  <img src="<%- url_for(`/img/${sponsor.img}`) %>" style="width: 120px;">
</a>
<%_ } _%>
